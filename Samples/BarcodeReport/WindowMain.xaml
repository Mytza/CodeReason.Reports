<Window x:Class="BarcodeReport.WindowMain"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:CodeReason.Reports.Controls;assembly=CodeReason.Reports"
    Title="CodeReason Reports - Barcode Report" Height="600" Width="600" WindowState="Maximized" Activated="Window_Activated">
    <Grid>
        <Button x:Name="cmdPDF" Height="20" Width="40" VerticalAlignment="Top" HorizontalAlignment="Left" Content="To PDF" Click="CmdPDF_OnClick"/>
        <c:BusyDecorator Name="busyDecorator" IsBusyIndicatorHidden="False">
            <DocumentViewer Name="documentViewer" Margin="0,22.388,0,0" />
        </c:BusyDecorator>
    </Grid>
</Window>
